                    <img src="/imgs/belem.jpg" alt="imagem area da cidade de Belém">
                </div>
                <div class="col-lg-6 about-content">
                    <h2 class="about-text">Um evento inedito!</h2>
                    <p class="about-text">
                        <%= informacao.descricao %>
                    </p>
                    <p class="about-text"></p>
                </div>
            </div>
        </div>
    </section>


    <!-- local e data do evento-->
    <section id="local">

        <div class="container-fluid">
            <div class="row">

                <div class="col-md-6 Localização">
